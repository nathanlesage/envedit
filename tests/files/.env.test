# General information about the application
APP_ENV=production
APP_DEBUG=false # Set this to true to output additional debug information
APP_KEY=
APP_TITLE=EnvEdit #This is the title of our app
APP_URL=http://localhost

# Data about how logs should be created
LOG_STORAGE=daily
LOG_FILES=30
LOG_LEVEL=info

# Information about the database
DB_CONNECTION=sqlite
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=database.sqlite
DB_USERNAME=null
DB_PASSWORD=null
DB_PREFIX=null

# Driver settings (only for laravel)
CACHE_DRIVER=file
SESSION_DRIVER=file
QUEUE_DRIVER=sync

# Redis information (only used if drivers are set to redis)
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mailing settings
MAIL_DRIVER=smtp
MAIL_HOST=mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
